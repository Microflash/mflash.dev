    "gridsome": "^0.7.19",
    "gridsome-plugin-remark-prismjs-all": "^0.3.5",
    "gridsome-remark-figure-caption": "^1.1.3",
    "remark-admonitions": "^1.2.1",
    "vue-fuse": "^2.2.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@microflash/remarkability": "^0.8.5",
    "@microflash/spritely": "^1.3.0",
