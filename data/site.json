{
  "name": "Microflash",
  "description": "Reflections on design and development by Naiyer Asif",
  "url": "https://mflash.dev",
  "favicon": "https://raw.githubusercontent.com/Microflash/microflash.github.io/release/src/favicon.png",
  "maintainer": "Naiyer Asif",
  "gatid": "UA-143076148-1",
  "tocPattern": "Table of Contents",
  "searchConfig": {
    "file": {
      "dir": "static",
      "name": "search.json"
    },
    "options": {
      "shouldSort": true,
      "includeMatches": true,
      "tokenize": true,
      "matchAllTokens": true,
      "threshold": 0.4,
      "location": 0,
      "distance": 600,
      "maxPatternLength": 32,
      "minMatchCharLength": 1,
      "keys": ["title", "blurb"]
    }
  }
}
